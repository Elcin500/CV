@model ResumeEditViewModel

@{
    Layout = null;
}

<!doctype html>
<html class="no-js" lang="en">

<!-- Mirrored from new.uouapps.com/remoticv/resume(edit-mode).html by HTTrack Website Copier/3.x [XR&CO'2014], Sun, 19 May 2019 06:20:44 GMT -->
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="author" content="M_Adnan" />
    <!-- Document Title -->
    <title>Resume / CV HTML5 Template</title>

    <!-- StyleSheets -->
    <link rel="stylesheet" href="~/assets/css/ionicons.min.css">
    <script src="https://kit.fontawesome.com/bd705139b3.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
    <link rel="stylesheet" href="~/assets/css/bootstrap/bootstrap.min.css">
    <link rel="stylesheet" href="~/assets/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/assets/css/main.css">
    <link rel="stylesheet" href="~/assets/css/style.css">
    <link rel="stylesheet" href="~/assets/css/responsive.css">

    <!-- FontsOnline -->
    <link href="https://fonts.googleapis.com/css?family=Raleway:100,200,300,400,500,600,700,800,900" rel="stylesheet">

    <!-- JavaScripts -->
    <script src="~/assets/js/vendors/modernizr.js"></script>
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
</head>
<body>

    <!-- Page Wrapper -->
    <div id="wrap">

        <!-- Header -->
        <header> </header>
        <!-- End Header -->
        <!-- Content -->
        <div id="content" class="resune-edit">
            <div class="resume">
                <div class="container">

                    <!-- TOP HEAD -->
                    <div class="top-head">
                        <div class="row">
                            <div class="col-sm-6">
                                <h4>Admin/ Resume </h4>
                                <a asp-controller="dashboard" asp-action="index">Admine qayit</a>
                            </div>
                        </div>
                    </div>

                    <!-- Resume -->
                    <div class="row">

                        <!-- Sidebar -->
                        <div class="col-md-4">
                            <div class="side-bar">

                                <!-- Professional Details -->
                                <h5 class="tittle">Professional Details</h5>
                                <form asp-action="ProfessionalDetails" method="post">
                                    <ul class="personal-info">
                                        <li>
                                            <p>
                                                    <select name="title" style="margin-bottom:10px;" class="form-control" asp-items="ViewBag.AppInfos"></select>
                                                    @*<input class="form-control" name="title" type="text" placeholder="Başlıq">*@
                                                
                                                <input class="form-control" name="text" type="text" placeholder="Mətni">
                                            </p>
                                        </li>
                                        <li> <button type="submit" class="btn"><i class="fa fa-plus"></i>Elave et</button> </li>
                                    </ul>
                                </form>

                                <!-- Attachments -->
                                <h5 class="tittle">Attachments</h5>
                                <div class="attach bor-btm padding-25">
                                    <ul>
                                        <li>
                                            <p><img src="~/assets/images/pdf-icon.jpg" alt=""> Curriculum-Vitae.pdf <a href="#."><i class="fa fa-cloud-download"></i></a> <a href="#."><i class="fa fa-eye"></i></a></p>
                                        </li>
                                        <li>
                                            <p><img src="~/assets/images/word-icon.jpg" alt=""> COE.docx <a href="#."><i class="fa fa-cloud-download"></i></a> <a href="#."><i class="fa fa-eye"></i></a></p>
                                        </li>
                                    </ul>
                                    <p>
                                        <span>
                                            <input class="form-control" type="text" placeholder="Title">
                                        </span>
                                    </p>
                                    <a href="#" class="btn"><i class="fa fa-plus"></i> Add Attachment</a>
                                </div>

                                <!-- Social Profiles -->
                                <h5 class="tittle">Social Profiles</h5>
                                <form class="social-icon bor-btm padding-25" method="post" asp-action="SocialLinks">
                                    <ul>
                                        <li>
                                            <i class="fa fa-facebook"></i>
                                            <input value="@Model.AppInfo.FacebookLink" name="facebookLink" class="form-control" type="text" placeholder="facebook.com/billgates">
                                        </li>
                                        <li>
                                            <i class="fa fa-twitter"></i>
                                            <input value="@Model.AppInfo.TwitterLink" name="twitterLink"  class="form-control" type="text" placeholder="twitter.com/billgates">
                                        </li>
                                        <li>
                                            <i class="fa fa-instagram"></i>
                                            <input value="@Model.AppInfo.InstagramLink" name="instagramLink"  class="form-control" type="text" placeholder="instagram.com/billgates">
                                        </li>
                                        <li>
                                            <i class="fa fa-linkedin"></i>
                                            <input value="@Model.AppInfo.Linkedin" name="linkedin"  class="form-control" type="text" placeholder="linkedin.com/billgates">
                                        </li>
                                        <li>
                                            <i class="fa fa-github"></i>
                                            <input value="@Model.AppInfo.GithubLink" name="githubLink"  class="form-control" type="text" placeholder="github.com/billgates">
                                        </li>
                                        <li> 
                                            <button type="submit" class="btn">Duzelis et</button>
                                        </li>

                                    </ul>
                                </form>

                            </div>
                        </div>

                        <!-- MAIN CONTENT -->
                        <div class="col-md-8">
                            <div class="inside-sec">
                                <!-- BIO AND SKILLS -->
                                <h5 class="tittle">Bio & Skills</h5>
                                <div class="bio-info padding-30">
                                    <!-- Bio -->
                                    <form asp-action="PersonBio" method="post">
                                        <textarea class="form-control text-dark" name="bio" placeholder="Write your description here">@Model.Person.Bio</textarea>
                                        <button type="submit" style="margin-top:7px;" class="btn">Düzəliş et</button>
                                    </form>

                                    <div class="skills margin-top-50">
                                        <form asp-action="SkillAdd" method="post">

                                            <!-- SKILLS -->
                                            <h6>Add A Skill</h6>

                                            <!-- Sound Engineering -->
                                            <div class="row">

                                                <!-- Categories -->
                                                <div class="col-sm-6">
                                                    <select name="category" class="selectpicker">
                                                        <option>Hard Skill</option>
                                                        <option>Soft Skill</option>
                                                    </select>
                                                </div>

                                                <!--  Skills -->
                                                <div class="col-sm-6">
                                                    <input type="text" name="name" class="form-control" placeholder="Adi">
                                                </div>
                                            </div>


                                            <!-- Skill Info -->
                                            <div class="skill-type-info">
                                                <label>
                                                    Bacariq Derecesi
                                                    <input type="text" name="percent" class="form-control" placeholder="0">
                                                </label>
                                                <label>
                                                    Bacariq Izahi
                                                    <textarea class="form-control text-dark" name="description" placeholder=""></textarea>
                                                </label>
                                            </div>
                                            <!-- ADD MORE-->
                                            <button type="submit" class="btn"><i class="fa fa-plus"></i> Elave et</button>
                                        </form>
                                    </div>
                                </div>
                            </div>

                            <!-- Professional Experience -->
                            <div class="inside-sec margin-top-30">
                                <!-- Professional Experience -->
                                <h5 class="tittle">Professional Experience</h5>
                                <div class="padding-30 exp-history">

                                    <!-- ADD Professional -->
                                    <form asp-action="ExperienceAdd" method="post" id="experienceForm" enctype="multipart/form-data">

                                        <div class="exp">
                                            <div class="row">
                                                <div class="col-sm-7">
                                                    <label>
                                                        Tarix
                                                        <input type="text" name="years" class="form-control" placeholder="YYYY - YYYY">
                                                    </label>
                                                    <label>
                                                        İş Adı
                                                        <input type="text" name="jobTitle" class="form-control" placeholder="">
                                                    </label>
                                                    <label>
                                                        İş sahəsi
                                                        <input type="text" name="jobArea" class="form-control" placeholder="">
                                                    </label>
                                                    <label>
                                                        Yeri
                                                        <input type="text" name="location" class="form-control" placeholder="">
                                                    </label>
                                                </div>

                                                <!-- UPLOAD IMG -->
                                                <div class="col-sm-5">
                                                    <label class="btn-file"  id="logoFile-viewer">
                                                        <i class="fa fa-camera"></i> Click to attach logo
                                                        <input name="logo" id="logoFile" type="file" style="display: none;">
                                                    </label>
                                                </div>

                                                <!-- ADD DETAILS -->
                                                <div class="col-sm-12">
                                                    <label>
                                                        Details
                                                        <textarea name="description" class="form-control text-dark" placeholder=""></textarea>
                                                    </label>
                                                    <!-- ADD MORE-->
                                                    <button type="submit" class="btn margin-top-20"><i class="fa fa-plus"></i>Elave et</button>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>

                            <!-- Academic Background -->
                            <div class="inside-sec margin-top-30" >
                                <!-- Academic Background -->
                                <h5 class="tittle">Academic Background</h5>
                                <div class="padding-30 exp-history">

                                    <form asp-action="StudyAdd" id="studyForm" method="post"  enctype="multipart/form-data">

                                        <!-- ADD EXP -->
                                        <div class="exp">
                                            <div class="row">
                                                <div class="col-sm-7">
                                                    <label>
                                                        Tarix
                                                        <input type="text" name="years" class="form-control" placeholder="YYYY - YYYY">
                                                    </label>
                                                    <label>
                                                        Adı
                                                        <input type="text" name="name" class="form-control" placeholder="">
                                                    </label>
                                                    <label>
                                                        Sahə və ya İxtisas
                                                        <input type="text" name="studyArea" class="form-control" placeholder="">
                                                    </label>
                                                    <label>
                                                        Yer
                                                        <input type="text" name="location" class="form-control" placeholder="">
                                                    </label>
                                                </div>

                                                <!-- UPLOAD IMG -->
                                                <div class="col-sm-5">
                                                    <div id="diploma-viewer"  class="alert alert-success d-none" role="alert"></div>
                                                    <label class="btn-file">
                                                        <i class="fa fa-camera"></i> Click to add diploma
                                                        <input type="file" name="diploma" id="diplomaFile" style="display: none;">
                                                    </label>
                                                </div>

                                                <!-- ADD DETAIL -->
                                                <div class="col-sm-12">
                                                    <label>
                                                        Ətraflı
                                                        <textarea name="description" class="form-control text-dark" placeholder=""></textarea>
                                                    </label>
                                                    <!-- ADD MORE -->
                                                    <button type="submit" class="btn margin-top-20"><i class="fa fa-plus"></i>Elavə et</button>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>

                            <!-- Save Info -->
                            <div class="save-info"> <a asp-area="" asp-controller="aboutMe" asp-action="index" class="btn">Sayta Keç</a> </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- End Content -->
        <!-- Footer -->
        <footer class="footer">
            <div class="rights">
                <p>© Copyright 2016 MotiCV. All right reserved.</p>
            </div>
        </footer>
        <!-- End Footer -->

    </div>
    <!-- End Page Wrapper -->
    <!-- JavaScripts -->
    <script src="~/assets/js/vendors/jquery/jquery.min.js"></script>
    <script src="~/assets/js/vendors/bootstrap.min.js"></script>
    <script src="~/assets/js/vendors/owl.carousel.min.js"></script>
    <script src="~/assets/js/main.js"></script>

    <script>
        $('#experienceForm').on("keyup keypress", function (e) {
                var keycode = e.keycode || e.which;
                if (keycode == 13) {
                e.preventDefault();
                    return false;
                }
            });

        $('#logoFile').change(function (e) {

                let reader = new FileReader();
                reader.addEventListener("load", function () {
                    $('#logoFile-viewer').css({
                    'background-image': `url(${reader.result})`
                })
            }, false);

                reader.readAsDataURL(e.target.files[0]);


        });



        $('#studyForm').on("keyup keypress", function (e) {
            var keycode = e.keycode || e.which;
            if (keycode == 13) {
                e.preventDefault();
                return false;
            }
        });

        $('#diplomaFile').change(function (e) {

            let reader = new FileReader();
            reader.addEventListener("load", function () {
                $('#diploma-viewer').removeClass('d-none');
                $('#diploma-viewer').text(`Fayl Secildi`);
            }, false);

            reader.readAsDataURL(e.target.files[0]);


        });

    </script>

</body>

